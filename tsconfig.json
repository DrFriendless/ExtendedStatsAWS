{
  "compilerOptions": {
    "module": "system",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "removeComments": false,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "es2016",
    "moduleResolution": "Node"
  },
  "include": [
    "./*.ts"
  ]
}
